<script>

    function closeAddDocTypeModal() {
        $("#addDocumentTypeModal").modal("hide");
    }

    function openAddDocType() {
        $("#addDocumentTypeModal").modal("show");
    }

    function addDocType() {
        let txtName = $("#txtATypeName");
        let txtDesc = $("#txtADescription");

        if (!CheckNullOrEmpty(txtName, "Document type name can not be empty or null"))
            return false;

        let action = '/DocumentType/AddDocType';
        let datasend = {
            DocumentTypeName: txtName.val(),
            Description: txtDesc.val(),
        };
        LoadingShow();
        PostDataAjax(action, datasend, function (response) {
            LoadingHide();
            if (response.rs) {
                toastr.success(response.msg, "Success");
                $("#addDocumentTypeModal").modal("hide");
                getDocumentTypes();
            }
            else {
                toastr.error(response.msg, "Warning");
            }
        });
    }

</script>